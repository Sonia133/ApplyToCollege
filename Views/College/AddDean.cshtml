@model College.Models.Dean
@{
    ViewBag.Title = "Add dean";
}

<h2>@ViewBag.Title</h2>
<h4>Pasul 2</h4>
@using (Html.BeginForm(actionName: "AddStep_2", controllerName: "College", method: FormMethod.Post))
{
    @Html.Label("Name", "Name: ")
    <br />
    @Html.TextBoxFor(b => b.Name, null, new { placeholder = "Name", @class = "formcontrol" })
    @Html.ValidationMessageFor(b => b.Name, "", new { @class = "text-danger" })
    <br />
    <br />
    @Html.Label("Email", "Email: ")
    <br />
    @Html.TextBoxFor(b => b.Email, null, new { placeholder = "Email", @class = "formcontrol" })
    @Html.ValidationMessageFor(b => b.Email, "", new { @class = "text-danger" })
    <br />
    <br />
    <button class="btn btn-success" type="submit">Add</button>
}
