@model College.Models.Student
@{
    ViewBag.Title = "Details";
}

<h2>@ViewBag.Title</h2>

<div class="panel-body">
    @Html.Label("Name", "Name")
    <br />
    <p>@Model.Name</p>
    <br />
    @Html.Label("Frequency", "Frequency")
    <br />
    <p>@Model.Frequency</p>
    <br />
    @Html.Label("Sat", "SAT grade")
    <br />
    <p>@Model.Sat</p>
    <br />
    @Html.Label("Email", "Email")
    <br />
    <p>@Model.Email</p>
    <br />
    @Html.Label("Badge", "Badge number")
    <br />
    <p>@Model.Badge</p>
</div>

@Html.ActionLink("Back to your profile", "Index", "Manage")